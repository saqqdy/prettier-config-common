{
	"$schema": "http://json.schemastore.org/prettierrc",
	"printWidth": 240,
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "none",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "avoid",
	"rangeStart": 0,
	"filepath": "",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "css",
	"endOfLine": "auto",
	"cursorOffset": -1,
	"overrides": [
		{
			"files": "*.test.js",
			"options": {
				"semi": false
			}
		},
		{
			"files": ["*.json5"],
			"options": {
				"singleQuote": false,
				"quoteProps": "preserve"
			}
		},
		{
			"files": ["*.yml"],
			"options": {
				"singleQuote": false
			}
		}
	]
}
